<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
          xmlns:customviews="clr-namespace:StockWatcher.CustomViews"
          x:Class="StockWatcher.CustomViews.WatcherCell"
          x:Name="this">
  <ContentView.Content>
      <StackLayout Orientation="Horizontal">
        <Label Text="{Binding Path=SecId, Source={x:Reference this}}"
               Margin="8, 0, 0, 0"
               VerticalOptions="CenterAndExpand" />

      </StackLayout>
  </ContentView.Content>
</ContentView>